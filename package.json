{
  "name": "business-hours-native",
  "version": "0.0.1",
  "description": "Native JavaScript business hour handler",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/bb26-spreadsheet.git"
  },
  "bugs": {
    "url": "https://github.com/istareatscreens/bb26-spreadsheet/issues"
  },
  "types": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "retail hours",
    "opening hours",
    "store hours",
    "business hours",
    "opening",
    "operation hours"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "sideEffects": false,
  "author": "Stephen Charbonneau",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/mocha": "5.2.7",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "chai": "4.2.0",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "istanbul": "0.4.5",
    "mocha": "6.2.2",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "source-map-support": "0.5.16",
    "ts-node": "8.5.4",
    "typescript": "3.7.4"
  }
}
