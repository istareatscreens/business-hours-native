{
  "name": "business-hours-native",
  "version": "1.2.0",
  "description": "Native JavaScript business hour and schedule handler",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/istareatscreens/business-hours-native.git"
  },
  "bugs": {
    "url": "https://github.com/istareatscreens/bbusiness-hours-native/issues"
  },
  "types": "lib",
  "files": [
    "lib"
  ],
  "keywords": [
    "retail hours",
    "opening hours",
    "store hours",
    "business hours",
    "opening",
    "operation hours"
  ],
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/*.ts",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "sideEffects": false,
  "author": "Stephen Charbonneau",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.0",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "chai": "4.2.0",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "istanbul": "0.4.5",
    "mocha": "7.1.1",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.0.1",
    "prettier": "2.0.4",
    "sinon": "9.0.2",
    "source-map-support": "0.5.17",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "dependencies": {}
}
